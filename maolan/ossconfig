#pragma once
#include <string>
#include <maolan/config>


class OSSConfig : public Config
{
  public:
    OSSConfig();
    OSSConfig(const OSSConfig &config);

    int fd;
    int format;
    int samplerate;
    std::size_t frag;
    std::size_t fragSize;
    std::string device;
};
